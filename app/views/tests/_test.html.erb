<tr>
  <td align=left><%= test.title %></td>
  <td align=left><%= test_level(test) %></td>
  <td align=center><%= number_of_questions(test) %></td>
  <td><%= link_to 'show', test_path(test) %></td>
  <td><%= link_to 'edit', edit_test_path(test) %></td>
  <td>
    <%= link_to 'delete', test_path(test) , method: :delete, data: { confirm: 'Are you sure?'}%>
  </td>
  <td><%= button_to 'Start', start_test_path(test) %></td>
</tr>
